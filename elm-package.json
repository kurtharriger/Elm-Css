{
    "version": "2.0.0",
    "summary": "Use the full power of Elm to generate CSS.",
    "repository": "https://github.com/adam-r-kowalski/Elm-Css.git",
    "license": "BSD3",
    "source-directories": [
        "."
    ],
    "exposed-modules": [
      "Css",
      "Css.Border.Bottom",
      "Css.Border.Left",
      "Css.Border.Right",
      "Css.Border.Style",
      "Css.Border.Top",
      "Css.Background",
      "Css.Border",
      "Css.Cursor",
      "Css.Dimension",
      "Css.Display",
      "Css.Flex",
      "Css.Float",
      "Css.Font",
      "Css.Gradient",
      "Css.ListStyle",
      "Css.Margin",
      "Css.Outline",
      "Css.Padding",
      "Css.Position",
      "Css.Shadow",
      "Css.Text",
      "Css.Transform2",
      "Css.Transform3",
      "Css.Util"
    ],
    "native-modules": false,
    "dependencies": {
        "coreytrampe/elm-vendor": "1.0.2 <= v < 2.0.0",
        "elm-lang/core": "2.1.0 <= v < 3.0.0",
        "evancz/elm-html": "3.0.0 <= v < 4.0.0"
    },
    "elm-version": "0.15.0 <= v < 0.16.0"
